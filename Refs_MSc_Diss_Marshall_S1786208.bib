@article{BCDMS:Deuteron,
issn = {0370-2693},
abstract = {We present results on a high statistics study of the nucleon structure functions F 2 ( x , Q 2 ) and R = σ L / σ T measured in deep inelastic scattering of muons on a deuterium target. The analysis is based on 8×10 5 events after all cuts, recorded at beam energies of 120, 200 and 280 GeV in the kinematic range 0.06⩽ × ⩽0.80 and 8GeV 2 ⩽ Q 2 ⩽260GeV 2 . Scaling violations observed in the data are in agreement with predictions of perturbative QCD and allow to determine the QCD mass scale parameter Λ.},
journal = {Physics Letters B},
pages = {592--598},
volume = {237},
publisher = {Elsevier B.V.},
number = {3},
year = {1990},
title = {A high statistics measurement of the deuteron structure functions F 2( x, Q 2) and R from deep inelastic muon scattering at high Q 2},
language = {eng},
author = {Benvenuti, A.C. and Bollini, D. and Bruni, G. and Navarria, F.L. and Argento, A. and Lohmann, W. and Piemontese, L. and Strachota, J. and Zavada, P. and Baranov, S. and Genchev, V.I. and Golutvin, I.A. and Kiryushin, Yu.T. and Krivokhizhin, V.G. and Kukhtin, V.V. and Lednicky, R. and Nemecek, S. and Reimer, P. and Savin, I.A. and Telyukov, E. and Smirnov, G.I. and Sultanov, G. and Todorov, P. and Volodko, A.G. and Jamnik, D. and Kopp, R. and Meyer-Berkhout, U. and Staude, A. and Teichert, K.-M. and Tirler, R. and Voss, R. and Zupancic, C. and Cribier, M. and Feltesse, J. and Milsztajn, A. and Ouraou, A. and Sacquin, Y. and Smadja, G. and Virchaux, M.},
